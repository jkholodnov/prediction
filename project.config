[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/jkholodnov
[access "refs/for/refs/*"]
	read = group user/jkholodnov
	push = group user/jkholodnov
	label-Code-Review = -2..+2 group user/jkholodnov
	label-Verified = -1..+1 group user/jkholodnov
	rebase = group user/jkholodnov
	removeReviewer = group user/jkholodnov
	submit = group user/jkholodnov
[access "refs/heads/*"]
	read = group user/jkholodnov
	create = group user/jkholodnov
	push = +force group user/jkholodnov
	pushMerge = group user/jkholodnov
[access "refs/tags/*"]
	push = +force group user/jkholodnov
	pushTag = group user/jkholodnov
	pushSignedTag = group user/jkholodnov
